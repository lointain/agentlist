{
  "name": "agentlist-worker-js",
  "version": "1.0.0",
  "description": "AgentList JavaScript/TypeScript Worker - HTTP + SSE 协议的 LangGraph 执行器",
  "type": "module",
  "engines": {
    "node": "^18.19.0 || >=20.16.0"
  },
  "license": "MIT",
  "main": "./dist/main.mjs",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/agentlist.git"
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "tsc",
    "dev": "tsx src/main.mts --dev",
    "start": "node dist/main.mjs",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "@hono/zod-validator": "^0.2.2",
    "@langchain/core": "^1.0.0",
    "@langchain/langgraph": "^1.0.0",
    "@langchain/langgraph-checkpoint": "^1.0.0",
    "dotenv": "^16.4.7",
    "hono": "^4.5.4",
    "tsx": "^4.19.3",
    "uuid": "^10.0.0",
    "winston": "^3.17.0",
    "winston-console-format": "^1.0.8",
    "zod": "^3.25.76 || ^4",
    "pg": "^8.12.0",
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/uuid": "^10.0.0",
    "@types/pg": "^8.11.10",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5 || ^5.4.5",
    "vitest": "^3.1.2"
  }
}